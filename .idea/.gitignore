# IDEs and editors
.idea/                 # IntelliJ/GoLand directory
.vscode/               # Visual Studio Code settings
*.iml                  # IntelliJ module configuration files
*.swp                  # Temporary files from text editors (e.g., Vim)

# Allow .editorconfig for code style consistency across IDEs
!.editorconfig

# Operating system files
.DS_Store              # macOS
Thumbs.db              # Windows

# Go dependencies
/vendor/               # If using 'go mod vendor'

# Build artifacts
/bin/                  # Go binaries
/build/                # Other build directories, if any
*.exe                  # Windows executables
*.dll                  # Windows libraries
*.so                   # Shared libraries
*.dylib                # macOS libraries

# Testing and profiling
*.out                  # Test output files
*.test                 # Test binaries generated by Go

# Version control system temporary files
.svn/
.hg/
.lock-wscript
.wafpickle-N

# Code coverage
coverage.out

# Other project-specific directories
/shelf/
/workspace.xml
/httpRequests/
/dataSources/
/dataSources.local.xml
